{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "@aenext": [
        "packages/index.ts"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "packages/**/*",
    "types.d.ts"
  ]
}
